{
  "name": "frc-challenge-lambda-functions",
  "version": "1.0.0",
  "description": "AWS Lambda functions for FRC Programming Challenge Platform",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "deploy": "npm run deploy:serverless",
    "deploy:dev": "npm run build && npm run deploy:serverless:dev",
    "deploy:prod": "npm run build && npm run deploy:serverless:prod",
    "deploy:serverless": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js deploy",
    "deploy:serverless:dev": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js deploy --stage dev",
    "deploy:serverless:prod": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js deploy --stage prod",
    "remove": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js remove",
    "remove:dev": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js remove --stage dev",
    "remove:prod": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js remove --stage prod",
    "seed": "node -r dotenv/config dist/scripts/seedData.js",
    "test": "node -r dotenv/config node_modules/.bin/jest",
    "test:api": "node scripts/testApi.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "info": "node -r dotenv/config ./node_modules/serverless/bin/serverless.js info --stage dev"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.658.1",
    "@aws-sdk/client-dynamodb": "^3.658.1",
    "@aws-sdk/lib-dynamodb": "^3.658.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.10.0",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.8.1",
    "serverless-webpack": "^5.14.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^6.0.1"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "frc",
    "robotics",
    "education",
    "typescript"
  ],
  "author": "FRC Challenge Platform",
  "license": "MIT"
}
