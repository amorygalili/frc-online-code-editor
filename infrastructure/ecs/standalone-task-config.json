{
  "taskDefinition": "frc-challenge-runtime",
  "cluster": "frc-challenge-cluster",
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "count": 1,
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "${PRIVATE_SUBNET_1}",
        "${PRIVATE_SUBNET_2}"
      ],
      "securityGroups": [
        "${CHALLENGE_RUNTIME_SECURITY_GROUP}"
      ],
      "assignPublicIp": "DISABLED"
    }
  },
  "overrides": {
    "containerOverrides": [
      {
        "name": "wpilib-editor",
        "environment": [
          {
            "name": "CHALLENGE_ID",
            "value": "${CHALLENGE_ID}"
          },
          {
            "name": "USER_ID",
            "value": "${USER_ID}"
          },
          {
            "name": "SESSION_ID",
            "value": "${SESSION_ID}"
          },
          {
            "name": "SESSION_TIMEOUT",
            "value": "${SESSION_TIMEOUT_MINUTES}"
          },
          {
            "name": "IDLE_TIMEOUT",
            "value": "${IDLE_TIMEOUT_MINUTES}"
          }
        ],
        "cpu": "${CPU_OVERRIDE}",
        "memory": "${MEMORY_OVERRIDE}"
      }
    ],
    "taskRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${TASK_ROLE_NAME}",
    "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${EXECUTION_ROLE_NAME}"
  },
  "tags": [
    {
      "key": "Environment",
      "value": "${ENVIRONMENT}"
    },
    {
      "key": "Project",
      "value": "frc-challenge-site"
    },
    {
      "key": "Component",
      "value": "challenge-session"
    },
    {
      "key": "UserId",
      "value": "${USER_ID}"
    },
    {
      "key": "ChallengeId",
      "value": "${CHALLENGE_ID}"
    },
    {
      "key": "SessionId",
      "value": "${SESSION_ID}"
    },
    {
      "key": "CreatedBy",
      "value": "lambda-session-manager"
    }
  ],
  "enableExecuteCommand": true,
  "propagateTags": "TASK_DEFINITION"
}
